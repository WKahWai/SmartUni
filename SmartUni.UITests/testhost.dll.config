<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- 
      Configure Selenium grid settings http://www.seleniumhq.org/projects/grid/
    -->
    <!--
      Choose on of the following drivers:
      InternetExplorer
      Chrome
      Firefox
    -->
    <add key="DriverToUse" value="Chrome" />
    <add key="TargetUrl" value="https://smartuni.azurewebsites.net/Home/SetLanguage?culture=en-US&amp;returnUrl=%2F" />
    <add key="ImplicitlyWait" value="30" />
    <add key="PageLoadTimeout" value="60" />
	  <add key="Headless" value="true"/>
  </appSettings>
</configuration>